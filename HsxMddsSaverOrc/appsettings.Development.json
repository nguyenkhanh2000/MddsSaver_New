{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceSetting": {
    "Name": "HsxMddsService",
    "Version": "1.0.0",
    "Namespace": "HsxMdds"
  },
  "KafkaLogger": {
    "Targets": {
      "Target1": {
        "Template": "{'DateTime':'{date:format=dd-MM-yyyy HH:mm:ss.fff}','Service':'{appName}','Host':'{hostName}','IP':'{iPv4s[0]}','Level':'{level:uppercase=true:truncate=short}','Logger':'{logger}','Message':'{message}'}",
        "Topic": "log-mdds"
      }
    },
    "Rules": [
      {
        "WriteTo": "Target1"
      }
    ],
    "BootstrapServers": "10.26.7.104:9092, 10.26.7.105:9092, 10.26.7.106:9092",
    "AppName": "HsxMddsSaverOracle"
  },
  "RabbitMQ": {
    "HostName": "10.26.2.158",
    "Port": 5672,
    "Username": "agent",
    "Password": "agent",
    "QueueName": "QueueHsxMddsSaver",
    "ExchangeName": "ExchangeHsxMddsSaver",
    "RoutingKey": "RoutingHsxMddsSaver",
    "BatchSize": 5000,
    "TimeDelay": 500,
    "PrefetchCount": 5000
  },
  "Redis": {
    "Endpoints": "10.26.7.84:6379,abortConnect=false,connectTimeout=3000,syncTimeout=3000,connectRetry=3",
    "KeyAppName_Consume": "HSXS5_0RD",
    "KeyAppName_Proc": "HSXS5.0"
  }
}
